@model List<Organization>;
<partial name="_UserNav" model= "ViewBag.UserObj"/>
 <section id="services" class="services">

      <div class="container" data-aos="fade-up">

        <header class="section-header">
          <p>All Orgnizations you can work with</p>
        </header>

        <div class="row gy-4">
        @foreach(var org in Model){
            <div class="col-lg-4 col-md-6" data-aos="fade-up" data-aos-delay="200">
            <div class="service-box blue">
              <i class="bi bi-building icon"></i>
              <h3>@org.Name</h3>
              <a href="/orgnaization/@org.OrganizationId" class="read-more"><span>Read More</span> <i class="bi bi-arrow-right"></i></a>
            </div>
          </div>
        }

          

        </div>

      </div>

    </section>